@model IEnumerable<OdeToBikes.Models.ManufacturerListViewModel>

<div id="manufacturerList">
    @foreach (var item in Model)
    {
        <div>
            <h4>@item.Name</h4>
            <div>@item.Location</div>
            <div>Models: @item.CountOfModels</div>
            <hr />
        </div>
    }
</div>
